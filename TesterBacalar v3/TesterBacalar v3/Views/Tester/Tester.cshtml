<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Panel</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    @Styles.Render("~/Content/TesterStyle.css") <!-- Тут заміниш на свій файл стилю-->
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <h3>Привіт!</h3>
    <h3>@ViewBag.Who</h3>
    <div class="row">
        <div class="col-xs-3 col-md-12">

            <table class="table">

                <tr>
                    <th>Іd </th>
                    <th>Назва</th>
                    <th>Предмет</th>


                </tr>

                @foreach (TesterBacalar_v3.Tests b in ViewBag.Tests)
                {
                    <tr>
                        <td data-label="Іd Користувача"><p>@b.test_id</p></td>
                        <td><p>@b.test_name</p></td>
                        <td><p>@b.subject_name</p></td>
                        <

                        <td>
                            <input type="submit" class="btn btn-default" href="/Tester/bbb" />
                            @Html.ActionLink("Пройти тест", "Question", "Tester", null, new { @class = "roundedButton" })
                        </td>
                    </tr>
                }
            </table>
        </div>
    </div>
</body>
</html>

